<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /api
RewriteRule ^(apache2|bind9|named|dhcpd|tftpd-hpa)/get/(enable|active) /api/service-get-$2.cgi?service=$1 [L]
RewriteRule ^(apache2|bind9|named|dhcpd|tftpd-hpa)/set/enable/(true|false) /api/service-set-enable.cgi?service=$1&state=$2 [L]
RewriteRule ^(apache2|bind9|named|dhcpd|tftpd-hpa)/set/active/(start|stop|restart) /api/service-set-active.cgi?service=$1&state=$2 [L]
RewriteRule ^(team|judge|print|web|pc2|cds|servers|all)/(get|state)/(mac|version)/?$ /api/$1-$2-$3.cgi  [L]
RewriteRule ^(team|judge|print|web|pc2|cds|servers)/([0-9]*)/(get|set|state)/(mac|version|ip)/?$ /api/$1-$3-$4.cgi?id=$2  [L]
RewriteRule ^(team|judge|print|web|pc2|cds|servers|all)/(live|count)/?$ /api/$1-$2.cgi  [L]
#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule . /api/error404.php [R=302,L]
</IfModule>

